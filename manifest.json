{
    "name": "Testing Code Gen",
    "description": "An extension to generate testing code from the browser",
    "version": "1.0",
    "manifest_version": 3,
    "devtools_page": "devtools.html",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content-scripts.js"]
      }
    ],
    "permissions": [
      "storage"
    ]
  }